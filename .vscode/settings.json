{
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit"
    },
    "eslint.options": {
      "extensions": [".html", ".js", ".ts", ".tsx"]
    },
    "eslint.validate": ["javascript", "typescript", "typescriptreact", "html"],
    "files.eol": "auto"
}